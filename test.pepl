(arr:int[]) = []
(i:int) = 0
<<"Enter in \"10\" numbers:"
while(i < 10){
    (hold:string)
    >>hold
    arr += [(hold->int)]
    ++i
}
<<arr
int[] sort(int[] arr){
    (sortarr:int[])
    while((#arr) > 0){
        (min:int) = arr[0]
        i = 0
        while(i < (#arr)){
            if((arr$i) < min){
                min = (arr$i)
            }
            ++i
        }
        sortarr += [(?min)]
        arr -= min
    }
    => sortarr
}
<<(sort{(?arr)})